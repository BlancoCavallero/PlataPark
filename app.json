{
  "expo": {
    "name": "PlataPark",
    "slug": "platapark",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/plata_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fblancocavallero.platapark"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fblancocavallero.platapark"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "008f26e2-eef7-4f3c-bccd-68d083b61875"
      }
    },
    "owner": "fblancocavallero"
  }
}
